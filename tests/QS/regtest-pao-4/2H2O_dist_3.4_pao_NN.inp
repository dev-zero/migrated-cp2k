&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    &QS
      LS_SCF
    &END QS
    &MGRID
      CUTOFF 400
      REL_CUTOFF 60
    &END MGRID
    &LS_SCF
      MAX_SCF     25
      EPS_SCF     1.0E-8
      EPS_FILTER  1.0E-20
      S_PRECONDITIONER  NONE
      REPORT_ALL_SPARSITIES OFF
      PURIFICATION_METHOD TRS4
      EXTRAPOLATION_ORDER  1
      &PAO
        MAX_PAO 1
        PARAMETERIZATION EXP
        &MACHINE_LEARNING
          METHOD NEURAL_NETWORK
          TOLERANCE 0.001
          TRAINING_FILE 2H2O_training.txt
        &END MACHINE_LEARNING
      &END PAO
    &END
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    ! dist 3.4
    &COORD
      O  -1.464   0.099   0.300  mol1
      H  -1.956   0.624  -0.340  mol1
      H  -1.797  -0.799   0.206  mol1
      O   1.838   0.154  -0.510  mol2
      H   2.363   0.494   0.220  mol2
      H   0.920   0.173  -0.198  mol2
    &END COORD
    &CELL
      ABC 8.0 8.0 8.0
    &END CELL
    &TOPOLOGY
      &CENTER_COORDINATES
      &END
    &END TOPOLOGY
    &KIND O
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE
      PAO_BASIS_SIZE 4
      &PAO_POTENTIAL
        BETA 0.8
        WEIGHT 1.0
      &END PAO_POTENTIAL
    &END KIND
    &KIND H
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE
      PAO_BASIS_SIZE 4
      &PAO_POTENTIAL
        BETA 0.8
        WEIGHT 1.0
      &END PAO_POTENTIAL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT 2H2O_dist_3.4_pao_NN
  RUN_TYPE ENERGY
&END GLOBAL
